# ===========================================
# NEUROPLAN BACKEND - CONFIGURACIÓN TEMPORAL
# ===========================================

# Base de datos (temporal - sin PostgreSQL)
DATABASE_URL="postgresql://postgres@localhost:5432/neuroplan?schema=public"

# Autenticación JWT
JWT_SECRET="neuroplan-jwt-secret-key-2024-change-in-production"
JWT_EXPIRES_IN="24h"

# Servidor
PORT=3001
NODE_ENV="development"
FRONTEND_URL="http://localhost:8080"
ALLOWED_ORIGINS="http://localhost:8080,http://localhost:5173,http://localhost:3000"

# AWS Services (mock mode)
AWS_REGION="eu-west-1"
AWS_ACCESS_KEY_ID="mock_key"
AWS_SECRET_ACCESS_KEY="mock_secret"
AWS_S3_BUCKET="neuroplan-demo-bucket"

# Servicios externos (mock mode)
CLAUDE_API_KEY="mock_claude_key"
ELEVENLABS_API_KEY="mock_elevenlabs_key"
N8N_URL="http://localhost:5678"

# Configuración de archivos
MAX_FILE_SIZE=10485760
UPLOAD_PATH="./uploads"
